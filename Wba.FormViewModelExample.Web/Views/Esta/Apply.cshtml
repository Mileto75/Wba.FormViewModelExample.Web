@model EstaApplyViewModel

<h2>Apply</h2>
<div class="row">
    <div class="col-md-4">
        <form asp-controller="Esta" asp-action="Process" method="post">
            <div class="form-group">
                <label asp-for="ArrivalDate"></label>
                <input asp-for="ArrivalDate" class="form-control">
            </div>
            <div class="form-group">
                <label asp-for="Email"></label>
                <input asp-for="Email" class="form-control">
            </div>
            <div class="form-group">
                <label asp-for="Telephone"></label>
                <input asp-for="Telephone" class="form-control">
            </div>
            <div class="form-group">
                <label asp-for="ZipCode"></label>
                <input asp-for="ZipCode" class="form-control">
            </div>
            <div class="form-group">
                <label asp-for="StreetAndNumber"></label>
                <input asp-for="StreetAndNumber" class="form-control">
            </div>
            <div class="form-group">
                <label asp-for="Municipality"></label>
                <input asp-for="Municipality" class="form-control">
            </div>
            <div class="form-group">
                <label asp-for="Region"></label>
                <input asp-for="Region" class="form-control">
            </div>
            <button class="btn btn-info" type="submit">Apply</button>
        </form>
    </div>
</div>

